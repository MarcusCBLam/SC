<%
	meta title: prefix_brand(@products.name),
	description: strip_html(@products.short_description),
	keywords: @products.keywords.gsub(" ", ","),
	reverse: true,
	canonical: request.original_url
%>
<div class="header_bc">
<nav class="breadcrumbs" role="menubar" aria-label="breadcrumbs">
	<li class="unavailable"  aria-disabled="true">>></li>
    <li role="menuitem"><a href="/">Home</a></li>
    <li role="menuitem"><a href="/products">Products</a></li>
    <li role="menuitem" class="current"><a href="#"><%= prefix_brand(@products.name) %></a></li>
</nav>
</div>
<div id="product_image">
  <%= render_partial 'products/product_partials/product_image' %>
</div>
<div id="product_thumbnails">
  <%= render_partial 'products/product_partials/product_thumbnails' %>
</div>
<div id="product_summary">
  <%= render_partial 'products/product_partials/product_summary' %>
</div>
<div id="product_applications">
  <%= render_partial 'products/product_partials/product_applications' %>
</div>
<div id="product_sections">
  <%= render_partial 'products/product_partials/product_sections' %>
</div>
<div id="product_shortcuts">
  <%= render_partial 'products/product_partials/product_shortcuts' %>
</div>
<div id="product_share">
  <%= render_partial 'products/product_partials/product_share' %>
</div>
<div id="product_buy_now">
  <%= render_partial 'products/product_partials/product_buy_now' %>
</div>



