
<%= cache ["product_square", product, website, compare, I18n.locale] do %>

<% 
		this_url = product_url(product)
%>

<div id="<%= product.to_param %>" >
	<table >
		<tr><td>
	<% if product.image %>
		
		<%= link_to(image_tag(product.image.url(:medium)), this_url) %>
		
	<% end %>
		</td></tr>
	</table>
	<h3><%= link_to(product.name, this_url) %></h3>

</div>

<% end %>